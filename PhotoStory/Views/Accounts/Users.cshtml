@model  IEnumerable<PhotoStory.Model.Account.User>

@{
    ViewBag.Title = "Users";
}

<h2>Users</h2>

<table>
    <thead>
        <tr>
            <th>
                Username
            </th>
            <th>
                Number of photos
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var u in Model) {
            <tr>
                <td>
                    @u.UserName
                </td>
                <td>
                    0
                </td>
            </tr>
        }
    </tbody>
    
</table>